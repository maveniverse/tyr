<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>eu.maveniverse.maven.parent</groupId>
    <artifactId>parent</artifactId>
    <version>41</version>
  </parent>

  <groupId>eu.maveniverse.maven.njord</groupId>
  <artifactId>tyr</artifactId>
  <version>0.1.0-SNAPSHOT</version>
  <packaging>pom</packaging>

  <name>${project.groupId}:${project.artifactId}</name>
  <description>Maveniverse Tyr.</description>

  <url>https://www.maveniverse.eu/tyr</url>

  <licenses>
    <license>
      <name>EPL-2.0</name>
      <url>https://www.eclipse.org/legal/epl-2.0/</url>
    </license>
  </licenses>

  <modules />

  <scm>
    <connection>scm:git:git@github.com:maveniverse/tyr.git</connection>
    <developerConnection>scm:git:git@github.com:maveniverse/tyr.git</developerConnection>
    <tag>HEAD</tag>
    <url>https://github.com/maveniverse/tyr</url>
  </scm>

  <issueManagement>
    <system>Github</system>
    <url>https://github.com/maveniverse/tyr/issues</url>
  </issueManagement>

  <ciManagement>
    <system>Github</system>
    <url>https://github.com/maveniverse/tyr/actions</url>
  </ciManagement>

  <properties>
    <project.build.outputTimestamp>2025-07-30T21:01:13Z</project.build.outputTimestamp>

    <!--
    Build time: latest stable Maven and Java 21+
    Run time: current Maven (3.9+) and Java 8+
    -->
    <maven.compiler.release>8</maven.compiler.release>
    <requireRuntimeMavenVersion.range>[3.9,)</requireRuntimeMavenVersion.range>

    <!-- Dependency versions -->
    <version.maveniverseShared>0.1.8</version.maveniverseShared>
    <version.maven>3.9.11</version.maven>
    <version.resolver>1.9.24</version.resolver>
    <version.mima>2.4.32</version.mima>
    <version.slf4j>1.7.36</version.slf4j>
  </properties>

</project>
